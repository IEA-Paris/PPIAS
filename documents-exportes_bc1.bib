@article{linebaugh_enclosures_2010,
	title = {Enclosures from the {Bottom} {Up}},
	volume = {2010},
	issn = {0163-6545},
	url = {https://doi.org/10.1215/01636545-2010-007},
	doi = {10.1215/01636545-2010-007},
	abstract = {This essay integrates two themes: the enclosure of land, or of other resources, as a physical mechanism of privatization; and the technique of historical investigation known as writing history “from the bottom up.” Building on the work of Elinor Ostrom, the 2009 Nobel laureate in economics for her work on the commons, the essay criticizes the powerful and influential 1968 article “The Tragedy of the Commons” by the biologist Garrett Hardin. It demonstrates how Hardin's central argument is derived from the ideas of a nineteenth-century Oxford professor responding to a protracted series of militant actions by a small community in Oxfordshire that sought to defend its commons from enclosure. The acts of resistance exerted by these Oxfordshire commoners were among the first events uncovered and written about by members of the Ruskin College History Workshop who pioneered history “from below” in England during the early 1970s.},
	number = {108},
	urldate = {2023-03-09},
	journal = {Radical History Review},
	author = {Linebaugh, Peter},
	month = oct,
	year = {2010},
	pages = {11--27},
	file = {Snapshot:C\:\\Users\\gmarmin\\Zotero\\storage\\QHQX5WF2\\Enclosures-from-the-Bottom-Up.html:text/html},
