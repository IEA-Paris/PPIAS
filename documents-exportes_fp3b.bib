	urldate = {2022-05-30},
	journal = {International Journal of Medical Informatics},
	author = {Marques, Gonçalo and Drissi, Nidal and Díez, Isabel de la Torre and de Abajo, Beatriz Sainz and Ouhbi, Sofia},
	month = mar,
	year = {2021},
	keywords = {Apps, Attitudes, COVID-19, GHQ-12, Mental health, Mobile health},
	pages = {104369},
}
